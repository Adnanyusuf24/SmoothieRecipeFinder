{
    "version": 2,
    "routes": [
      {
        "src": "/Ecommerce(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/SmoothieRecipeFinder(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ],
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      } 
    ]
  }
